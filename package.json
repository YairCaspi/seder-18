{
  "name": "i18n-seder",
  "version": "1.0.0",
  "description": "A lightweight local JSON-based translation editor for i18n projects. Opens a local UI to manage and edit your translation files directly from your project.",
  "main": "src/cli.js",
  "bin": {
    "seder": "./src/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/USERNAME/i18n-seder.git"
  },
  "homepage": "https://github.com/USERNAME/i18n-seder#readme",
  "bugs": {
    "url": "https://github.com/USERNAME/i18n-seder/issues"
  },
  "scripts": {
    "start": "node src/cli.js --dir ./src/example",
    "start:example": "node src/cli.js --dir ./src/example --main en --ignore fr.json",
    "build": "cd src/client && npm run build"
  },
  "files": [
    "src/cli.js",
    "src/client/dist/",
    "src/backend/index.js",
    "src/backend/translations.js",
    "package.json",
    "README.md"
  ],
  "keywords": [
  "i18n",
  "translation",
  "internationalization",
  "localization",
  "editor",
  "translations editor",
  "json translations",
  "i18n tool",
  "translate",
  "react",
  "vue",
  "seder",
  "seder18"
],
  "author": "",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "minimist": "^1.2.8",
    "yargs": "^17.7.2"
  }
}
